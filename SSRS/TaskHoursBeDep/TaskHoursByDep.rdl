  <DataSets>
    <DataSet Name="Gip">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <CommandText>declare @empCtId varbinary(512) = 0x010040D9D13AF3634AACB514CB30B54F2CAE004BC5116E5FCC734388387FAF0125CB7D
select 
tp_ID as EmpId,
nvarchar1 as EmpName
from AllUserData with (nolock)
where tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' 
and tp_IsCurrent=1 and tp_DeleteTransactionId=0x
and tp_ContentTypeId = @empCtId
and bit1 = 1
and int3 = 32
union all
select
d.tp_ID,
d.nvarchar1
from AllUserDataJunctions j with (nolock)
left join AllUserData d with (nolock) on j.tp_Id = d.tp_ID 
and d.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' 
and d.tp_IsCurrent = 1 
and d.tp_DeleteTransactionId = 0x
and d.tp_ContentTypeId = @empCtId
and d.bit1 = 1
where 
j.tp_DocId = '33547D89-2A74-41DD-99E5-8A9F7035E805' 
and j.tp_IsCurrentVersion = 1 
and j.tp_DeleteTransactionId = 0x</CommandText>
      </Query>
      <Fields>
        <Field Name="EmpId">
          <DataField>EmpId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EmpName">
          <DataField>EmpName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Contract">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@gip">
            <Value>=Parameters!gip.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
d1.tp_ID as ConId,
(d1.nvarchar3 + ' ' + d1.nvarchar1) as ConName
from AllUserData d1 with (nolock)
where d1.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7'
and d1.tp_IsCurrent = 1 
and d1.tp_DeleteTransactionId = 0x
and d1.int6 in (@gip)
Order by ConName Asc</CommandText>
      </Query>
      <Fields>
        <Field Name="ConId">
          <DataField>ConId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ConName">
          <DataField>ConName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Department">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <CommandText>select 
tp_ID as DepId,
nvarchar1 as DepName
from AllUserData with (nolock)
where tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' 
and tp_IsCurrent=1 and tp_DeleteTransactionId=0x 
and CONVERT(varchar(max), tp_ContentTypeId, 2) = N'0100CE4B4034AA87410EB92561C8318E2C16005639E40A0D3F6342A19FF44EB45B5A85' 
and bit1 = 1 
and bit5 = 1
order by DepName asc</CommandText>
      </Query>
      <Fields>
        <Field Name="DepId">
          <DataField>DepId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DepName">
          <DataField>DepName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
