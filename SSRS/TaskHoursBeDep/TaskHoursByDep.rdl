    <DataSet Name="Main">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@prj">
            <Value>=Parameters!prj.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dep">
            <Value>=Parameters!dep.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
main.*,
(case when asg.DepId is null then chk.DepId else asg.DepId end) as DepartmentId,
(case when asg.DepId is null then d2.nvarchar1 else d.nvarchar1 end) as DepartmentName,
(case when asg.Total is null then 0 else asg.Total end) as DurationAssign,
(case
when main.AssignToId = main.CheckUserId then 0
when chk.Total is null then 0 
else chk.Total 
end) as DurationCheck,
(case when dur.Total is null then 0 else dur.Total end) as Duration,
(dur.Total - main.DurationPlan) as DurationDiff,
dt.FactStartDate,
(case when main.VitroPlannerTaskStatusId = 6 then dt.FactEndDate else NULL end) as FactEndDate
from
(
select
t.ItemId,
c.tp_ID as ContractId,
c.nvarchar3 as ContractNum,
g.tp_ID as GIPId,
g.nvarchar1 as GIP,
t.Title,
t.AssignToId,
o.nvarchar1 as AssignedTo,
t.CheckUserId,
o2.nvarchar1 as CheckUser,
t.VitroPlannerTaskStartDatePlan,
t.VitroPlannerTaskEndDatePlan,
t.DurationPlan,
t.VitroPlannerTaskStatusId
from QRY_VITRO_PLANNER_TASKS t
left join QRY_VITRO_PLANNER_TASKS p on t.ObjectUniqueId = p.tp_DocId
left join AllUserData c with (nolock) on c.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7' and c.tp_IsCurrent=1 and c.tp_DeleteTransactionId=0x and c.tp_ID = p.VitroPlannerProject
left join AllUserData g with (nolock) on g.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and g.tp_IsCurrent=1 and g.tp_DeleteTransactionId=0x and c.int6 = g.tp_ID
left join AllUserData o with (nolock) on o.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o.tp_IsCurrent=1 and o.tp_DeleteTransactionId=0x and t.AssignToId = o.tp_ID
left join AllUserData o2 with (nolock) on o2.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o2.tp_IsCurrent=1 and o2.tp_DeleteTransactionId=0x and t.CheckUserId = o2.tp_ID
where 
t.TaskContentType = 1
and p.VitroPlannerProject in (@prj)
) main

left join
(
select
TaskId,
usr.OrgId,
o.int3 as DepId,
sum(res.WorkHours) as Total
from QRY_VITRO_TSK_STATUS_RES res
left join QRY_VITRO_PLANNER_TASKS_USER_ID usr on usr.UserId = res.UserId
left join AllUserData o with (nolock) on o.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o.tp_IsCurrent=1 and o.tp_DeleteTransactionId=0x and usr.OrgId = o.tp_ID
where res.WorkHours != 0 and (res."Date" &gt;= @startDate or @startDate is NULL) and (res."Date" &lt;= @endDate or @endDate is NULL) and o.int3 in (@dep)
group by TaskId, OrgId, o.int3
) asg on asg.TaskId = main.ItemId and asg.OrgId = main.AssignToId

left join
(
select
TaskId,
usr.OrgId,
o.int3 as DepId,
sum(res.WorkHours) as Total
from QRY_VITRO_TSK_STATUS_RES res
left join QRY_VITRO_PLANNER_TASKS_USER_ID usr on usr.UserId = res.UserId
left join AllUserData o with (nolock) on o.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o.tp_IsCurrent=1 and o.tp_DeleteTransactionId=0x and usr.OrgId = o.tp_ID
where res.WorkHours != 0 and (res."Date" &gt;= @startDate or @startDate is NULL) and (res."Date" &lt;= @endDate or @endDate is NULL) and o.int3 in (@dep)
group by TaskId, OrgId, o.int3
) chk on chk.TaskId = main.ItemId and chk.OrgId = main.CheckUserId

left join
(
select
TaskId,
sum(res.WorkHours) as Total
from QRY_VITRO_TSK_STATUS_RES res
where res.WorkHours != 0 and (res."Date" &gt;= @startDate or @startDate is NULL) and (res."Date" &lt;= @endDate or @endDate is NULL)
group by TaskId
) dur on dur.TaskId = main.ItemId

left join
(
select
TaskId,
Min(Date) as FactStartDate,
Max(Date) as FactEndDate
from QRY_VITRO_TSK_STATUS_RES
where WorkHours != 0
group by TaskId
) dt on dt.TaskId = main.ItemId
left join AllUserData d with (nolock) on d.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and d.tp_IsCurrent=1 and d.tp_DeleteTransactionId=0x and asg.DepId = d.tp_ID
left join AllUserData d2 with (nolock) on d2.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and d2.tp_IsCurrent=1 and d2.tp_DeleteTransactionId=0x and chk.DepId = d2.tp_ID
where 
asg.DepId is not null or chk.DepId is not null
option (recompile)</CommandText>
      </Query>
