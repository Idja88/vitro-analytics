  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@itemId">
            <Value>=Parameters!itemId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select
d.LeafName,
r.SOURCE_ITEM_ID,
(CASE WHEN r.SOURCE_LIST_ID = 'D0A26142-5468-44B3-9964-15BDDA509051' THEN 'Workflow'
WHEN r.SOURCE_LIST_ID = 'CDAF6BFD-6813-4FD3-BC9C-EAA6998E7544' THEN 'Task'
END) as WFType,
u2.nvarchar1 as Theme,
(CASE WHEN r.SOURCE_LIST_ID = 'D0A26142-5468-44B3-9964-15BDDA509051' THEN u2.nvarchar16
WHEN r.SOURCE_LIST_ID = 'CDAF6BFD-6813-4FD3-BC9C-EAA6998E7544' THEN u2.nvarchar13
END) as WFStatus,
u2.tp_Created,
u2.datetime4 as Completed
FROM AllDocs AS d WITH (NOLOCK)
left join AllUserData AS u with (nolock) on d.DoclibRowId = u.tp_ID and u.tp_ListId = d.ListId and u.tp_DeleteTransactionId=0x and u.tp_IsCurrent=1
right join REP_ENTITY_REF AS r WITH (NOLOCK) ON r.DEST_ID=d.Id and d.DoclibRowId = r.DEST_ITEM_ID
left join AllUserData AS u2 with (nolock) on u2.tp_ID=r.SOURCE_ITEM_ID and u2.tp_ListId=r.SOURCE_LIST_ID and u2.tp_DeleteTransactionId=0x and u2.tp_IsCurrent=1
WHERE d.DoclibRowId = @itemId and d.Type = 0 and d.DeleteTransactionId=0x</CommandText>
      </Query>
