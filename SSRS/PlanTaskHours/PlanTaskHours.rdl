    <DataSet Name="Main">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@prj">
            <Value>=Parameters!prj.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dep">
            <Value>=Parameters!dep.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@emp">
            <Value>=Parameters!emp.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
t.ID as TaskId,
t.VitroPlannerTaskParentId as TaskParentId,
t.Title as TaskTitle,
t.VitroPlannerTaskStatus as TaskStatus,
t.VitroPlannerTaskStartDatePlan as TaskStartDatePlan,
t.VitroPlannerTaskEndDatePlan as TaskEndDatePlan,
c.tp_ID as ContractId,
c.nvarchar3 as ContractNum,
ad1.tp_ID as AssignToId,
ad1.nvarchar1 as AssignToFullName,
ad2.tp_ID as DepartmentId,
ad2.nvarchar1 as Department,
ad3.tp_ID as CheckUserId,
ad3.nvarchar1 as CheckUserFullName,
ad4.tp_ID as CheckDepartmentId,
ad4.nvarchar1 as CheckDepartment,
pl.PlanDuration,
pl.PlanDays
from 
(
	select
	l.TASK_ID,
	ROUND(SUM(l.DURATION), 2) as PlanDuration,
	COUNT(l.DATE) as PlanDays
	from TSK_DURATION_PLAN_LOG l with (nolock) 
	where (l.DATE &gt;= @startDate or @startDate is NULL) and (l.DATE &lt;= @endDate or @endDate is NULL) 
	group by l.TASK_ID
) pl
left join QRY_VITRO_PLANNER_TASKS t on t.tp_DocId = pl.TASK_ID and t.ItemId = pl.TASK_ID
left join QRY_VITRO_PLANNER_TASKS p on t.ObjectUniqueId = p.tp_DocId and t.ObjectUniqueId = p.ItemId
left join AllUserData c with (nolock) on c.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7' and c.tp_IsCurrent=1 and c.tp_DeleteTransactionId=0x and c.tp_ID = p.VitroPlannerProject
left join AllUserData ad1 with (nolock) on t.AssignToId = ad1.tp_ID and ad1.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad1.tp_DeleteTransactionId = 0x and ad1.tp_IsCurrent = 1
left join  AllUserData ad2 with (nolock) on ad1.int3 = ad2.tp_ID and ad2.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad2.tp_DeleteTransactionId = 0x and ad2.tp_IsCurrent = 1
left join AllUserData ad3 with (nolock) on t.CheckUserId = ad3.tp_ID and ad3.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad3.tp_DeleteTransactionId = 0x and ad3.tp_IsCurrent = 1
left join  AllUserData ad4 with (nolock) on ad3.int3 = ad4.tp_ID and ad4.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad4.tp_DeleteTransactionId = 0x and ad4.tp_IsCurrent = 1
where
t.VitroPlannerTaskStatusId in (2, 3, 14, 20)
and t.TaskContentType not in (4, 6, 7, 8, 15, 16)
and p.VitroPlannerProject in (@prj)
and (ad2.tp_ID = (@dep) or @dep is null)
and (ad1.tp_ID = (@emp) or @emp is null)</CommandText>
      </Query>
