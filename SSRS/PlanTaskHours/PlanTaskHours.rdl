          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Sort">
          <DataField>Sort</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employee">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dep">
            <Value>=Parameters!dep.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @empCtId varbinary(512) = 0x010040D9D13AF3634AACB514CB30B54F2CAE004BC5116E5FCC734388387FAF0125CB7D
select 
ad1.tp_ID as UserId,
ad1.nvarchar1 as UserName,
1 as Sort
from AllUserData ad1 with (nolock)
where ad1.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE'
and ad1.tp_IsCurrent=1 and ad1.tp_DeleteTransactionId=0x
and tp_ContentTypeId = @empCtId
and ad1.bit1 = 1 
and (ad1.int3 in (@dep) or @dep is null)
union all
select
NULL,
N'ВСЕ',
0
order by Sort, UserName</CommandText>
      </Query>
      <Fields>
        <Field Name="UserId">
          <DataField>UserId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
