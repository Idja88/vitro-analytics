  <DataSets>
    <DataSet Name="Workload">
      <Query>
        <DataSourceName>WSSDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dep">
            <Value>=Parameters!dep.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@prj">
            <Value>=Parameters!prj.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
Date,
NULL as PlanValue,
Value,
OrgID,
FullName,
DepartmentID,
DepartmentName,
PositionName,
CenterCode,
NULL as ConstructionPlace,
NULL as ProjectTitle,
NULL as GIP,
NULL as DurationPlan,
NULL as ProjectCode,
NULL as ContractNumber,
datepart(yyyy, Date)as YearOfTheDate,
datepart(mm, Date)as MonthOfTheYear,
(datediff(ww,datediff(d,0,dateadd(m,datediff(m,7,date),0))/7*7,dateadd(d,-1,date))+1) as WeekOfTheMonth,
0 as SortColumn
from Vitro.dbo.QRY_VACATION_DATA
where Date between @startDate and @endDate and DepartmentID in (@dep)

union all

select
d.DATE,
d.DURATION,
NULL,
t.AssignToId,
o.nvarchar1 as FullName,
o2.tp_ID as DepartmentID,
o2.nvarchar1 as DepartmentName,
p.nvarchar1 as PositionName,
c.nvarchar3 as CenterCode,
t2.ParentTaskTitle,
t2.Title,
--t2.GIP,
ad3.nvarchar1,
t2.DurationPlan,
ad1.nvarchar35 as ProjectCode,
ad2.nvarchar3 as ContractNumber,
datepart(yyyy, d.Date)as YearOfTheDate,
datepart(m, d.Date)as MonthOfTheYear,
(datediff(ww,datediff(d,0,dateadd(m,datediff(m,7,d.DATE),0))/7*7,dateadd(d,-1,d.DATE))+1) as WeekOfTheMonth,
1
from
WSS_content.dbo.TSK_DURATION_PLAN_LOG d
left join WSS_Content.dbo.QRY_VITRO_PLANNER_TASKS t on d.TASK_ID = t.ItemId and d.TASK_ID = t.tp_DocId and d.TASK_ID = t.DocId

left join WSS_Content.dbo.AllUserData o with (nolock) on o.tp_ID = t.AssignToId and o.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o.tp_DeleteTransactionId = 0x and o.tp_IsCurrent = 1
left join WSS_Content.dbo.AllUserData o2 with (nolock) on o.int3 = o2.tp_ID and o2.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o2.tp_DeleteTransactionId = 0x and o2.tp_IsCurrent = 1
left join WSS_Content.dbo.AllUserData p with (nolock) on o.int2 = p.tp_ID and p.tp_ListId = 'CEFF7864-BF8B-4EFD-AD76-9351A229DF5B' and p.tp_DeleteTransactionId = 0x and p.tp_IsCurrent = 1
left join WSS_Content.dbo.AllUserData c with (nolock) on o.int8 = c.tp_ID and c.tp_ListId = '6ACD7F25-92F9-4D68-B013-FA81930C852A' and c.tp_DeleteTransactionId = 0x and c.tp_IsCurrent = 1

left join WSS_Content.dbo.QRY_VITRO_PLANNER_TASKS t2 on t2.ItemId = t.ObjectUniqueId
left join WSS_Content.dbo.AllUserData ad1 with (nolock) on ad1.tp_ListId = '2894DFC6-D9AE-4666-98AF-A5C45DF940C8' and ad1.tp_IsCurrent=1 and ad1.tp_DeleteTransactionId=0x and ad1.tp_ID = t2.VitroPlannerProject
left join WSS_Content.dbo.AllUserData ad2 with (nolock) on ad2.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7' and ad2.tp_IsCurrent=1 and ad2.tp_DeleteTransactionId=0x and ad2.tp_ID = ad1.int2
left join WSS_Content.dbo.AllUserData ad3 with (nolock) on ad3.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad3.tp_IsCurrent = 1 and ad3.tp_DeleteTransactionId = 0x and ad3.tp_ID = ad1.int3
where d.DATE between @startDate and @endDate and t.TaskContentType = 1 and o2.tp_ID in (@dep) and t2.VitroPlannerProject in (@prj)</CommandText>
      </Query>
