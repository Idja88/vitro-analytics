  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dep">
            <Value>=Parameters!dep.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
r.WorkHours,
t.ID,
t.Title,
ad1.tp_ID as UserId,
ad1.nvarchar1 as UserName,
ad4.tp_ID as DepartmentId,
ad4.nvarchar1 as DepartmentName,
t2.VitroPlannerProject as ContractId,
ad7.nvarchar3 as ContractNumber,
ad9.nvarchar11 as GIP,
100 as ProjectHoursType
from QRY_VITRO_TSK_STATUS_RES r
right join QRY_VITRO_PLANNER_TASKS t on t.ItemId=r.TaskId
left join QRY_VITRO_PLANNER_TASKS_USER_ID u on r.UserId = u.UserId
left join AllUserData ad1 with (nolock) on ad1.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad1.tp_IsCurrent=1 and ad1.tp_DeleteTransactionId=0x and ad1.tp_ID = u.OrgId
left join AllUserData ad4 with (nolock) on ad4.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad4.tp_IsCurrent=1 and ad4.tp_DeleteTransactionId=0x and ad4.tp_ID = ad1.int3
left join QRY_VITRO_PLANNER_TASKS t2 on t.ObjectUniqueId = t2.ItemId
left join AllUserData ad7 with (nolock) on ad7.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7' and ad7.tp_IsCurrent=1 and ad7.tp_DeleteTransactionId=0x and ad7.tp_ID = t2.VitroPlannerProject
left join AllUserData ad8 with (nolock) on ad8.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad8.tp_IsCurrent=1 and ad8.tp_DeleteTransactionId=0x and ad8.tp_ID = ad7.int6
left join AllUserData ad9 with (nolock) on ad9.tp_ListId = 'B4C516DB-A26D-48FA-A322-4780A5D4BE91' and ad9.tp_IsCurrent=1 and ad9.tp_DeleteTransactionId=0x and ad9.tp_ID = ad8.int1
where r.Type = 100 and r.Date between @startDate and @endDate and ad4.tp_ID in (@dep) --and t2.VitroPlannerProject in (@prj)

union all

select
r.WorkHours,
NULL,
NULL,
ad1.tp_ID as UserId,
ad1.nvarchar1 as UserName,
ad4.tp_ID as DepartmentId,
ad4.nvarchar1 as DepartmentName,
NULL as ContractId,
r.Title as ContractNumber,
NULL as GIP,
0 as ProjectHoursType
from QRY_VITRO_TSK_STATUS_RES r
left join QRY_VITRO_PLANNER_TASKS_USER_ID u on r.UserId = u.UserId
left join AllUserData ad1 with (nolock) on ad1.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad1.tp_IsCurrent=1 and ad1.tp_DeleteTransactionId=0x and ad1.tp_ID = u.OrgId
left join AllUserData ad4 with (nolock) on ad4.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and ad4.tp_IsCurrent=1 and ad4.tp_DeleteTransactionId=0x and ad4.tp_ID = ad1.int3
where r.Type != 100 and r.Date between @startDate and @endDate  and ad4.tp_ID in (@dep)

Option (Recompile)
</CommandText>
      </Query>
