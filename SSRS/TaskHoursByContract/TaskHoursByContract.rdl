    <DataSet Name="Gip">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <CommandText>declare @empCtId varbinary(512) = 0x010040D9D13AF3634AACB514CB30B54F2CAE004BC5116E5FCC734388387FAF0125CB7D
select 
tp_ID as EmpId,
nvarchar1 as EmpName
from AllUserData with (nolock)
where tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' 
and tp_IsCurrent=1 and tp_DeleteTransactionId=0x
and tp_ContentTypeId = @empCtId
and bit1 = 1
and int3 = 32
union all
select
d.tp_ID,
d.nvarchar1
from AllUserDataJunctions j with (nolock)
left join AllUserData d with (nolock) on j.tp_Id = d.tp_ID 
and d.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' 
and d.tp_IsCurrent = 1 
and d.tp_DeleteTransactionId = 0x
and d.tp_ContentTypeId = @empCtId
and d.bit1 = 1
where 
j.tp_DocId = '33547D89-2A74-41DD-99E5-8A9F7035E805' 
and j.tp_IsCurrentVersion = 1 
and j.tp_DeleteTransactionId = 0x
Order by EmpName</CommandText>
      </Query>
      <Fields>
        <Field Name="EmpId">
          <DataField>EmpId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="EmpName">
          <DataField>EmpName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Contract">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@gip">
            <Value>=Parameters!gip.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@st">
            <Value>=Parameters!st.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
d1.tp_ID as ConId,
(d1.nvarchar3 + ' ' + d1.nvarchar1) as ConName,
DATEADD(hh, 6, d1.datetime4) as ConPlanStartDate,
DATEADD(hh, 6, d1.datetime5)  as ConPlanEndDate
from AllUserData d1 with (nolock)
where d1.tp_ListId = 'C58717A9-F22D-40C2-9D93-9C1B76CEFBF7'
and d1.tp_IsCurrent = 1 
and d1.tp_DeleteTransactionId = 0x
and d1.tp_ContentTypeId != 0x012000E2C8FCCA524FEE44BCE222CDEDB077200055F21CA92387DF4D98C21E11685341E7
and d1.int3 in (@st)
and d1.int6 in (@gip)
Order by ConName Asc</CommandText>
      </Query>
      <Fields>
        <Field Name="ConId">
          <DataField>ConId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ConName">
          <DataField>ConName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ConPlanStartDate">
          <DataField>ConPlanStartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ConPlanEndDate">
          <DataField>ConPlanEndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ContractStatus">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <CommandText>select
d1.tp_ID as StId,
d1.nvarchar1 as StName
from AllUserData d1 with (nolock)
where d1.tp_ListId = '716D0899-605C-4C05-B8F1-4A44D3552274'
and d1.tp_IsCurrent = 1 
and d1.tp_DeleteTransactionId = 0x
Order by StId Asc</CommandText>
      </Query>
      <Fields>
        <Field Name="StId">
