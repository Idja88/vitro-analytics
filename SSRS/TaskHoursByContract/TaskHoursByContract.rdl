  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>WSS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prj">
            <Value>=Parameters!prj.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;with cte as
(
 select
ID
,ItemId
,VitroPlannerTaskParentId
,cast(ID as varchar(max)) as HierarchyPath
,AssignToId
,CheckUserId
,TaskContentType
from QRY_VITRO_PLANNER_TASKS
where
VitroPlannerProject = @prj

union all

select
t.ID
,t.ItemId
,t.VitroPlannerTaskParentId
,cast(rt.HierarchyPath + '/' + cast(t.ID as varchar(max)) as varchar(max))
,t.AssignToId
,t.CheckUserId
,t.TaskContentType
FROM QRY_VITRO_PLANNER_TASKS t
inner join cte rt ON t.VitroPlannerTaskParentId = rt.ID
)

select
main.ID,
main.VitroPlannerTaskParentId,
tsk.Title,
tsk.TaskContentTypeName,
NULL as AssignedTo,
NULL as CheckUser,
tsk.DurationPlanMax as DurationPlanMax,
tsk.DurationPlan as DurationPlan,
main.DurationAssign as DurationAssign,
main.DurationCheck as DurationCheck,
main.Duration,
(main.Duration - tsk.DurationPlan) as DurationDiff,
(main.Duration - tsk.DurationPlanMax) as DurationDiffMax,
tsk.VitroPlannerTaskStartDatePlan,
tsk.VitroPlannerTaskEndDatePlan,
tsk.VitroPlannerTaskStartDate as FactStartDate,
tsk.VitroPlannerTaskEndDate as FactEndDate
from
(
select
cte.ID,
cte.VitroPlannerTaskParentId,
(CASE WHEN chk2.DurationCheck IS NULL THEN 0 ELSE chk2.DurationCheck END) as DurationCheck,
(CASE WHEN asg2.DurationAssign IS NULL THEN 0 ELSE asg2.DurationAssign END) as DurationAssign,
(CASE WHEN dur2.Duration IS NULL THEN 0 ELSE dur2.Duration END) as Duration
from cte

left join
(
	select
	t1.VitroPlannerTaskParentId,
	SUM(chk1.Total) as DurationCheck
	from cte t1
	left join
	(
	select
	t2.ID,
	t2.VitroPlannerTaskParentId,
	t2.HierarchyPath,
	(case when t2.AssignToId = t2.CheckUserId then 0 else sum(res1.WorkHours) end) as Total
	--SUM(res1.WorkHours) as Total
	from cte t2
	left join QRY_VITRO_PLANNER_TASKS_USER_ID usr1 on usr1.OrgId = t2.CheckUserId
	left join QRY_VITRO_TSK_STATUS_RES res1 on t2.ItemId = res1.TaskId and res1.UserId = usr1.UserId
	where t2.TaskContentType = 1
	and res1.WorkHours != 0
	and (res1.Date &gt;= @startDate or @startDate is NULL) 
	and (res1.Date &lt;= @endDate or @endDate is NULL)
	group by t2.ID, t2.VitroPlannerTaskParentId, t2.HierarchyPath, t2.AssignToId, t2.CheckUserId
	) chk1 on chk1.HierarchyPath like t1.HierarchyPath + '%'
	group by t1.VitroPlannerTaskParentId
) chk2 on chk2.VitroPlannerTaskParentId = cte.ID

left join
(
	select
	t3.VitroPlannerTaskParentId,
	SUM(asg1.Total) as DurationAssign
	from cte t3
	left join
	(
	select
	t4.ID,
	t4.VitroPlannerTaskParentId,
	t4.HierarchyPath,
	SUM(res2.WorkHours) as Total
	FROM cte t4
	left join QRY_VITRO_PLANNER_TASKS_USER_ID usr2 on usr2.OrgId = t4.AssignToId
	left join QRY_VITRO_TSK_STATUS_RES res2 on t4.ItemId = res2.TaskId and res2.UserId = usr2.UserId
	where t4.TaskContentType = 1 
	and res2.WorkHours != 0
	and (res2.Date &gt;= @startDate or @startDate is NULL) 
	and (res2.Date &lt;= @endDate or @endDate is NULL)
	group by t4.ID, t4.VitroPlannerTaskParentId, t4.HierarchyPath
	) asg1 on asg1.HierarchyPath like t3.HierarchyPath + '%'
	group by t3.VitroPlannerTaskParentId
) asg2 on asg2.VitroPlannerTaskParentId = cte.ID

left join
(
	select
	t5.VitroPlannerTaskParentId,
	SUM(dur1.Total) as Duration
	from cte t5
	left join
	(
	select
	t6.ID,
	t6.VitroPlannerTaskParentId,
	t6.HierarchyPath,
	SUM(res3.WorkHours) as Total
	from cte t6
	left join QRY_VITRO_TSK_STATUS_RES res3 on t6.ItemId = res3.TaskId
	where t6.TaskContentType = 1 
	and res3.WorkHours != 0
	and (res3.Date &gt;= @startDate or @startDate is NULL) 
	and (res3.Date &lt;= @endDate or @endDate is NULL)
	group by t6.ID, t6.VitroPlannerTaskParentId, t6.HierarchyPath
	) dur1 on dur1.HierarchyPath like t5.HierarchyPath + '%'
	group by t5.VitroPlannerTaskParentId
) dur2 on dur2.VitroPlannerTaskParentId = cte.ID
where TaskContentType != 1
) main
left join QRY_VITRO_PLANNER_TASKS tsk on main.ID = tsk.ID

union all

select
main.ID,
main.VitroPlannerTaskParentId,
tsk.Title,
tsk.TaskContentTypeName,
o.nvarchar1 as AssignedTo,
o2.nvarchar1 as CheckUser,
tsk.DurationPlanMax as DurationPlanMax,
tsk.DurationPlan as DurationPlan,
main.DurationAssign,
main.DurationCheck,
main.Duration,
(main.Duration - tsk.DurationPlan) as DurationDiff,
(main.Duration - tsk.DurationPlanMax) as DurationDiffMax,
tsk.VitroPlannerTaskStartDatePlan,
tsk.VitroPlannerTaskEndDatePlan,
main.FactStartDate,
(CASE WHEN tsk.VitroPlannerTaskStatusId = 6 THEN main.FactEndDate ELSE NULL END) as FactEndDate
from
(
select
cte.ID,
cte.VitroPlannerTaskParentId,
(CASE
WHEN cte.AssignToId = cte.CheckUserId THEN 0
WHEN chk.Total IS NULL THEN 0 
ELSE chk.Total
END) as DurationCheck,
--(CASE WHEN chk.Total IS NULL THEN 0 ELSE chk.Total END) as DurationCheck,
(CASE WHEN asg.Total IS NULL THEN 0 ELSE asg.Total END) as DurationAssign,
--((CASE WHEN asg.Total IS NULL THEN 0 ELSE asg.Total END) + (CASE WHEN chk.Total IS NULL THEN 0 ELSE chk.Total END)) as Duration,
(CASE WHEN dur.Total IS NULL THEN 0 ELSE dur.Total END) as Duration,
dts.FactStartDate,
dts.FactEndDate
from cte

left join
(
	select
	t2.ID,
	SUM(res1.WorkHours) as Total
	from cte t2
	left join QRY_VITRO_PLANNER_TASKS_USER_ID usr1 on usr1.OrgId = t2.CheckUserId
	left join QRY_VITRO_TSK_STATUS_RES res1 on t2.ItemId = res1.TaskId and res1.UserId = usr1.UserId
	where t2.TaskContentType = 1
	and res1.WorkHours != 0
	and (res1.Date &gt;= @startDate or @startDate is NULL) 
	and (res1.Date &lt;= @endDate or @endDate is NULL)
	group by t2.ID
) chk on chk.ID = cte.ID

left join
(
	select
	t4.ID,
	SUM(res2.WorkHours) as Total
	FROM cte t4
	left join QRY_VITRO_PLANNER_TASKS_USER_ID usr2 on usr2.OrgId = t4.AssignToId
	left join QRY_VITRO_TSK_STATUS_RES res2 on t4.ItemId = res2.TaskId and res2.UserId = usr2.UserId
	where t4.TaskContentType = 1 
	and res2.WorkHours != 0
	and (res2.Date &gt;= @startDate or @startDate is NULL) 
	and (res2.Date &lt;= @endDate or @endDate is NULL)
	group by t4.ID
) asg on asg.ID = cte.ID

left join
(
	select
	t6.ID,
	SUM(res3.WorkHours) as Total
	from cte t6
	left join QRY_VITRO_TSK_STATUS_RES res3 on t6.ItemId = res3.TaskId
	where t6.TaskContentType = 1 
	and res3.WorkHours != 0
	and (res3.Date &gt;= @startDate or @startDate is NULL) 
	and (res3.Date &lt;= @endDate or @endDate is NULL)
	group by t6.ID
) dur on dur.ID = cte.ID

left join
(
	select
	t7.ID,
	Min(Date) as FactStartDate,
	Max(Date) as FactEndDate
	from cte t7
	left join  QRY_VITRO_TSK_STATUS_RES res3 on t7.ItemId = res3.TaskId
	where res3.WorkHours != 0
	group by t7.ID
) dts on dts.ID = cte.ID
where cte.TaskContentType = 1
) main
left join QRY_VITRO_PLANNER_TASKS tsk on main.ID = tsk.ID
left join AllUserData o on o.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o.tp_IsCurrent=1 and o.tp_DeleteTransactionId=0x and tsk.AssignToId = o.tp_ID
left join AllUserData o2 on o2.tp_ListId = 'D944262C-298C-493E-85ED-D7664EF7DEAE' and o2.tp_IsCurrent=1 and o2.tp_DeleteTransactionId=0x and tsk.CheckUserId = o2.tp_ID
option (recompile)</CommandText>
      </Query>
